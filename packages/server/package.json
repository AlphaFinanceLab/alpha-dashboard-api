{
  "name": "@alphadashboard/server",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "clear": "rm -rf ./build",
    "build:types": "npx tsc --project ./tsconfig.json",
    "build:js": "npx babel src --out-dir build --copy-files --extensions '.ts,tsx,.js,jsx'",
    "build:js:watch": "npx babel src --watch --out-dir build --copy-files --extensions '.ts,tsx,.js,jsx'",
    "build:js:watch_only": "npx babel src --watch --out-dir build --copy-files --extensions '.ts,tsx,.js,jsx' --skip-initial-build",
    "build": "yarn clear && yarn build:types && yarn build:js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "start": "NODE_ENV=production node ./build/index.js",
    "start:prod:debug": "NODE_ENV=production node --inspect-brk ./build/index.js",
    "start:dev": "NODE_ENV=development npx nodemon",
    "start:dev:debug": "export TS_NODE_FILES=true && node --inspect-brk node_modules/.bin/ts-node ./src/index.ts",
    "bsc:sync_events": "npx ts-node src/bsc/bsc_sync_events.ts",
    "bsc:sync_events:debug": "node --inspect-brk -r ts-node/register src/bsc/bsc_sync_events.ts",
    "eth:sync_events": "npx ts-node src/eth/eth_sync_events.ts",
    "eth:sync_events:debug": "node --inspect-brk -r ts-node/register src/eth/eth_sync_events.ts",
    "ethv2:sync_events": "npx ts-node src/ethV2/eth_v2_sync_events.ts",
    "ethv2:sync_events:debug": "node --inspect-brk -r ts-node/register src/ethV2/eth_v2_sync_events.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@alphadashboard/graphql": "1.0.0",
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "@prisma/client": "3.1.1",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "16.10.2",
    "axios": "0.22.0",
    "bignumber.js": "9.0.1",
    "ccxt": "1.57.45",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "date-fns": "2.24.0",
    "date-fns-tz": "1.1.6",
    "dotenv": "10.0.0",
    "ethereum-block-by-date": "1.4.2",
    "ethers": "5.4.7",
    "express": "4.17.1",
    "express-graphql": "0.12.0",
    "graphql": "15.6.0",
    "graphql-scalars": "1.10.1",
    "helmet": "4.6.0",
    "nodemon": "2.0.13",
    "prisma": "3.1.1",
    "typescript": "4.4.3",
    "web3": "1.6.0"
  },
  "devDependencies": {
    "ts-node": "10.2.1"
  }
}
